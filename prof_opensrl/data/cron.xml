<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
<record id="ir_cron_crm_unassign_overdue_deadline" model="ir.cron">
    <field name="name">CRM: Rimuovi assegnazione oltre 90gg dalla deadline</field>
    <field name="model_id" ref="crm.model_crm_lead"/>
    <field name="state">code</field>
    <field name="code">model.cron_unassign_overdue_deadline()</field>
    <field name="interval_type">days</field>
    <field name="interval_number">1</field>
    <!-- rimuovere questa riga: <field name="numbercall">-1</field> -->
    <field name="active">True</field>
    <field name="priority">3</field>
    <field name="nextcall" eval="(DateTime.now() + relativedelta(hours=2)).strftime('%Y-%m-%d %H:00:00')"/>
</record>

    </data>
</odoo>